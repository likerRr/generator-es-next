{
  "name": "generator-es6-module-boilerplate",
  "version": "1.0.0-beta.2",
  "description": "ES6 module generator for browser and nodejs",
  "license": "MIT",
  "repository": "likerRr/es6-module-boilerplate",
  "author": "Alexey Lizurchik <al.lizurchik@gmail.com> (http://likerRr.ru)",
  "engines": {
    "node": ">=6"
  },
  "man": "./README.md",
  "scripts": {
    "build": "npm run lint && webpack -p",
    "coverage": "nyc report --reporter=text-lcov",
    "coverage:report": "npm run coverage | codecov",
    "lint": "xo",
    "start": "webpack --watch",
    "test": "nyc ava",
    "test:watch": "npm run test -- -watch"
  },
  "files": [
    "generators"
  ],
  "browser": "dist/bundle.js",
  "main": "index.js",
  "keywords": [
    "es6",
    "boilerplate",
    "browser",
    "node",
    "starter"
  ],
  "dependencies": {
    "which": "^1.2.14",
    "yeoman-generator": "^1.1.1"
  },
  "devDependencies": {
    "xo": "^0.17.1"
  },
  "xo": {
    "esnext": true,
    "semicolon": true,
    "space": 2
  },
  "preferGlobal": true
}
