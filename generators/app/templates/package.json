{
  "name": "<%= moduleName %>",
  "version": "0.0.1",
  "description": "<%= moduleDescription %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "author": "<%= name %> <<%= email %>> (<%= website %>)",
  "engines": {
    "node": ">=6"
  },
  "man": "./README.md",
  "scripts": {
    "build": "npm run lint && webpack -p",
    "coverage": "nyc report --reporter=text-lcov",
    "coverage:report": "npm run coverage | codecov",
    "lint": "xo",
    "start": "webpack --watch",
    "test": "nyc ava",
    "test:watch": "npm run test -- -watch"
  },
  "files": [
    "index.js"
  ],
  "browser": "dist/bundle.js",
  "main": "index.js",
  "keywords": [
    "es6",
    "boilerplate",
    "browser",
    "node",
    "starter"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-loader": "^6.4.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-env": "^1.2.1",
    "babel-register": "^6.24.0",
    "codecov": "^2.0.1",
    "nyc": "^10.1.2",
    "webpack": "^2.3.2",
    "xo": "^0.18.1"
  },
  "xo": {
    "esnext": true,
    "semicolon": true,
    "space": 2
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "test.js"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "check-coverage": true,
    "sourceMap": false,
    "instrument": false
  }
}
