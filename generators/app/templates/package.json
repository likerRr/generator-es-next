{
  "name": "<%= moduleName %>",
  "version": "0.0.1",
  "description": "<%= moduleDescription %>",
  "license": "MIT",
  "repository": "<%= githubUsername %>/<%= moduleName %>",
  "author": "<%= name %> <<%= email %>> (<%= website %>)",
  "engines": {
    "node": ">=6"
  },
  "man": "./README.md",
  "scripts": {
    "build": "npm run lint && webpack -p",
    "lint": "xo",
    "start": "webpack --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:coverage": "npm run coverage:collect && codecov",
    "coverage:collect": "echo \"Error: no test specified\" && exit 1",
    "precommit": "test:coverage && npm run build",
    "prepublish": "npm run precommit"
  },
  "files": [
    "index.js"
  ],
  "browser": "dist/bundle.js",
  "main": "index.js",
  "module": "index.js",
  "keywords": [
    "es6",
    "boilerplate",
    "browser",
    "node",
    "starter"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-loader": "^6.4.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-env": "^1.2.1",
    "babel-register": "^6.24.0",
    "codecov": "^2.0.1",
    "webpack": "^2.3.2",
    "xo": "^0.18.1"
  },
  "xo": {
    "esnext": true,
    "semicolon": true,
    "space": 2
  }
}
